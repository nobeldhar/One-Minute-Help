<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomTextInputLayout">
        <attr name="font_type" format="enum">
            <enum name="Roboto-Regular" value="0" />
            <enum name="Roboto-Medium" value="1" />
            <enum name="Roboto-Bold" value="2" />
            <enum name="Roboto-Italic" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AutoGridLayout">
        <attr name="columnWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DotProgressBar">
        <attr name="color" format="color" />
        <attr name="count" format="integer" />
        <attr name="timeout" format="integer" />
    </declare-styleable>

    <!--<declare-styleable name="CustomTextInputLayout">
        <attr name="mainHintTextSize" format="dimension" />
    </declare-styleable>-->

</resources>